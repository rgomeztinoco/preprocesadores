.buscador
  display: flex
  justify-content: center
  align-items: center
  width: 100%
  height: 250px
  padding: 50px 25px
  background-image: url("../images/cover.jpg")
  background-size: cover
  background-position: center
  background-repeat: no-repeat
  &__input
    width: 100%
    height: 60px
    padding: 20px
    border: none
    border-radius: 30px
    f-size(var(--fuente-mediana))